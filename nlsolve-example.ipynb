   "source": [
    "using NLsolve\n",
    "\n",
    "function f!(F, x)\n",
    "    F[1] = 28.0 - x[1]^2 - x[1] * x[2]\n",
    "    F[2] = 21.0 - x[2]^2 - x[1] * x[2]\n",
    "end\n",
    "\n",
    "initial_x = [1.0, 1.0]\n",
    "result = nlsolve(f!, initial_x, autodiff = :forward)\n",
    "\n",
    "@info result"
   ]
